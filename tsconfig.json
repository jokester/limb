{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2022",
    "lib": ["ES2022"],
    "importHelpers": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "incremental": true,
    "pretty": true,
    "skipLibCheck": true,
    "jsxImportSource": "preact"
  },
  "include": ["demo/**/*.ts", "server/**/*.ts"]
}
